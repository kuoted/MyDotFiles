  "version": "0.2.0",
  "presentation": "group",
  "configurations": [
    {
      "name": "ROS: Launch via ssh",
      "type": "ros",
      "request": "launch",
      "target": "${workspaceFolder}/04_GEEP50_0002_AppFw/install/cupcake/lib/cupcake/cupcake",
    },
    {
      // Example launch of a python file
      "name": "Python: Current File (Integrated Terminal)",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "autoReload": {
        "enable": true
      },
      "envFile": "${workspaceFolder}/.vscode/env_launch_ros2"
    },
    // Example gdb launch of a ros executable
    {
      //launch debug via SSH on remote virtualbox,
      "name": "(gdb) Launch sunroof via ssh on Remote",
      "type": "cppdbg",
      "request": "launch",
      //  "preLaunchTask": "colcon: build(debug)",
      "program": "${workspaceFolder}/105_sunroof_workspace/install/ccu_bd_sunroof/lib/ccu_bd_sunroof/ccu_bd_sunroof",
      "args": [],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
      ],
      "envFile": "${workspaceFolder}/.vscode/env_launch_ros2.sunroof"
    },
    {
      //launch debug via SSH on remote virtualbox,
      "name": "(gdb) Launch vehicle_lock via ssh on Remote",
      "type": "cppdbg",
      "request": "launch",
      //  "preLaunchTask": "colcon: build(debug)",
      "program": "${workspaceFolder}/102_vehicle_lock_workspace/install/vehicle_lock/lib/vehicle_lock/vehicle_lock",
      "args": [],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
      ],
      "envFile": "${workspaceFolder}/.vscode/env_launch_ros2"
    },
    {
      //launch debug via SSH on remote virtualbox,
      "name": "(gdb) Launch elec_side_step via ssh on Remote",
      "type": "cppdbg",
      "request": "launch",
      //  "preLaunchTask": "colcon: build(debug)",
      "program": "${workspaceFolder}/20220609_2_elec_side_step_workspace/install/bdc_package/lib/bdc_package/bdc",
      "args": [],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
      ],
      "envFile": "${workspaceFolder}/.vscode/env_launch_ros2"
    },
  ],
}